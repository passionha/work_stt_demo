<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.byweb.stt.demo.cm.mapper.MemberMapper">
	<!-- 로그인 인증 위한 권한 코드 조회 -->
	<select id="authenticate" resultType="kr.byweb.stt.demo.cm.model.MemberVo">
		SELECT 
		       AUTH_CD
		FROM AIAS_MEMBER
		WHERE
	            ACNT_ID = #{acnt_id}
		    AND ACNT_PW = #{acnt_pw}
		    AND USE_YN = 'Y'
	</select>
</mapper>